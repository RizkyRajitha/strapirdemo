    "singularName": "wallpaper",
    "pluralName": "wallpapers",
    "displayName": "Wallpaper",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
