        "FOURK",
        "HD",
        "FHD"
      ],
      "required": true
    },
    "url": {
      "type": "string",
      "required": true
    },
    "addedBy": {
      "type": "string",
      "default": "RBFX",
      "required": true
    },
    "downloads": {
      "type": "integer",
      "required": false,
